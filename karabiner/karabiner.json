                            {
                                "from": {
                                    "key_code": "equal_sign",
                                    "modifiers": { "optional": ["caps_lock"] }
                                },
                                "to": [
                                    {
                                        "key_code": "equal_sign",
                                        "modifiers": ["left_shift"]
                                    }
                                ],
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "equal_sign",
                                    "modifiers": {
                                        "mandatory": ["shift"],
                                        "optional": ["caps_lock"]
                                    }
                                },
                                "to": [{ "key_code": "equal_sign" }],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Quit application by pressing command-q twice",
                        "manipulators": [
                            {
                                "conditions": [
                                    {
                                        "name": "command-q",
                                        "type": "variable_if",
                                        "value": 1
                                    }
                                ],
                                "from": {
                                    "key_code": "q",
                                    "modifiers": {
                                        "mandatory": ["command"],
                                        "optional": ["caps_lock"]
                                    }
                                },
                                "to": [
                                    {
                                        "key_code": "q",
                                        "modifiers": "left_command"
                                    }
                                ],
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "q",
                                    "modifiers": {
                                        "mandatory": ["command"],
                                        "optional": ["caps_lock"]
                                    }
                                },
                                "to": [
                                    {
                                        "set_variable": {
                                            "name": "command-q",
                                            "value": 1
                                        }
                                    }
                                ],
                                "to_delayed_action": {
                                    "to_if_canceled": [
                                        {
                                            "set_variable": {
                                                "name": "command-q",
                                                "value": 0
                                            }
                                        }
                                    ],
                                    "to_if_invoked": [
                                        {
                                            "set_variable": {
                                                "name": "command-q",
                                                "value": 0
                                            }
                                        }
                                    ]
                                },
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Change shift + delete to forward delete (rev 2)",
