                                        "modifiers": ["left_shift"]
                                    }
                                ],
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "semicolon",
                                    "modifiers": {
                                        "mandatory": ["shift"],
                                        "optional": ["caps_lock"]
                                    }
                                },
                                "to": [{ "key_code": "semicolon" }],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "escキーを押したときに、英数キーも送信する（vim用）",
                        "manipulators": [
                            {
                                "from": { "key_code": "escape" },
                                "to": [
                                    { "key_code": "escape" },
                                    { "key_code": "japanese_eisuu" }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
